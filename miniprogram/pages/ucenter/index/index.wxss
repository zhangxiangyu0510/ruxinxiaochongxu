  display: flex;
  justify-content: space-between;
  width: 685rpx;
  height: 104rpx;
  margin-bottom: 48rpx;
  background-image:linear-gradient(90deg, #C4D600 0%, #00B5E2 100%);
  border-radius: 16rpx;

}
.myself_buy_box .EN_number view{
  display: flex;
  align-items: center;
}
.myself_buy_box .EN_number image{
