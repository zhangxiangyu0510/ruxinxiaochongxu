  border-left: 2rpx solid rgba(255,255,255,.3);
  border-right: 2rpx solid rgba(255,255,255,.3);
  padding:0 0 0 36rpx;
  box-sizing: border-box;
  padding-right:36rpx;
}
.level_icon{
  height:32rpx;
  width:32rpx;
