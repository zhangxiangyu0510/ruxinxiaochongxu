.container {
  width: 100vw;
  height: 100vh;
  position: relative;
}

.tui-chatroom-navigatorbar{
  position: fixed;
  /*top: 0;*/
  flex-shrink: 0;
  width: 750rpx;
  height: 176rpx;
  z-index:999999;
  /* background-color: #006EFF; */
}
.calling_contain{
    padding-top:176rpx;
    position:relative;
}

.tui-chatroom-navigatorbar-back{
    position: absolute;
    width: 60rpx;
    height: 60rpx;
    left: 24rpx;
    bottom: 15rpx;
}

.conversation-title {
    position: absolute;
    width: 50%;
     white-space: nowrap;
     overflow: hidden;
     text-overflow: ellipsis;
    height: 88rpx;
    line-height: 56rpx;
    font-size: 36rpx;
    color: #FFFFFF;
    z-index:100;
    bottom: -20rpx;
    left: 50%;
    transform: translateX(-50%);
    align-items: center;
    text-align: center;
}

.message-list {
  flex: 1;
  width: 100vw;
  overflow-y: scroll;
  padding-bottom:176rpx;
}

.message-input {
  flex-shrink: 0;
  width: 100%;
  position: fixed;
  bottom: 0;
  left:0;
  right:0;
}
.calling {
  position: fixed;
  z-index: 199;
  top: 0;
  bottom: 0;
  right: 0;
}
.group-profile {
  top: 176rpx;
  left: 0;
  z-index: 8
}
