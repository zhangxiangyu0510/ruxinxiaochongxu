/* pages/ucenter/badgeDetail/badgeDetail.wxss */
page {
  min-height: 100%;
  background-color: #fafafa;
}
.badgeDetail{
  border-top:1rpx solid rgba(0, 0, 0, 0.1);
  background-color: #FFF;
}
.badgeDetail_top{
  width:100%;
  height:612rpx;
  text-align: center;
  overflow: hidden;
  background-image: url('data:image/svg+xml;base64,');
}
/* .badgeDetail_top .bageBackground{
  position: absolute;
  top: 0;
  left: 0;
  z-index: -1;
  width: 100%;
  height: 100%;
} */
.badgeDetail_top_logo{
  height:270rpx;
  width:270rpx;
  /* background: #000; */
  margin:72rpx auto 48rpx auto;
}
.badgeDetail_top_logo image{
  width: 100%;
  height: 100%;
  object-fit: contain;
}
.badgeDetail_top_main_text{
  font-family: PingFang SC;
  font-style: normal;
  font-weight: 600;
  font-size: 48rpx;
  line-height: 68rpx;
  color: #404040;
}
.badgeDetail_top_second_text{
  margin:32rpx 64rpx;
  font-family: PingFang SC;
  font-style: normal;
  font-weight: normal;
  font-size: 28rpx;
  line-height: 40rpx;
  color: #7F7F7F;
}
.badgeDetail_process_box{
  position: relative;
  display: flex;
  padding-top:82rpx;
  overflow: hidden;
  background-color: #fafafa;
  overflow-x: auto;

}
.linear{
  position: absolute;
  top: 82rpx;
  width: 80rpx;
  height: 12rpx;
  background: linear-gradient(90deg, #ffffff 0%, transparent 100%);
  z-index: 888;
}
.badgeDetail_process_box::-webkit-scrollbar{
  width: 0;
  height: 0;
  color: transparent;
}
.process_box{
  display: flex;
  align-items: center;
  margin-bottom:200rpx;
  height:12rpx;
  /* background: red; */
  background: #EEE;
  
}
.process-line{
  position: relative;
  display: flex;
  width: 100%;
  height: 12rpx;
}
.process-line .badge-line{
  width: 152rpx;
  height: 12rpx;
}
.process-line .badge-line .on-line{
  width: 0;
  height: 12rpx;
  border-radius: 40rpx;
}
.process-line .badge-spot{
  position: absolute;
  top: -6rpx;
  right: -12rpx;
  width: 24rpx;
  height: 24rpx;
  background: #A6A6A6;
  border-radius: 20px;
}
.badge-spot .spot-shadw{
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
  width: 48rpx;
  height: 48rpx;
  background: transparent;
  opacity: 0.15;
  border-radius: 20px;
}
.process-line .badge-text{
  position: absolute;
  top:60rpx;
  right: -60rpx;
  width: 120rpx;
  height: 64rpx;
  border-radius: 8rpx;
  border: 4rpx solid #EEEEEE;
  font-family: PingFang SC;
  font-style: normal;
  font-weight: 500;
  font-size: 28rpx;
  text-align: center;
  color: #A6A6A6;
}
.process-line .badge-text text{
  line-height: 64rpx;
}
.process_level{
  width:120rpx;
  height:64rpx;
  background: #FF5E00;
  border-radius: 8rpx;
  text-align: center;
  font-family: PingFang SC;
  font-style: normal;
  font-weight: 500;
  font-size: 28rpx;
  line-height: 64rpx;
  color: #FFFFFF;
}
.badgeDetail_process_text{
  margin:96rpx 0 64rpx 0;
  text-align: center;
  font-family: PingFang SC;
  font-style: normal;
  font-weight: 600;
  font-size: 40rpx;
  line-height: 56rpx;
  color: #404040;
}
.badgeDetail_process_reward{
  height:104rpx;
  background: #FFF;
  border-radius: 16rpx;
  margin:0 32rpx;
  text-align: center;
  line-height: 104rpx;
  font-family: PingFang SC;
  font-style: normal;
  font-weight: normal;
  font-size: 28rpx;
  color: #4A4A4A;
}

