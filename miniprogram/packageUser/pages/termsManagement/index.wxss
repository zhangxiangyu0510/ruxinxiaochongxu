/* packageUser/pages/termsManagement/index.wxss */
page{
    background: #FAFAFA;
    padding-top: constant(safe-area-inset-top); /* 兼容 iOS < 11.2 */
    padding-top: env(safe-area-inset-top); /* 兼容 iOS >= 11.2 */
    padding-bottom: constant(safe-area-inset-bottom); /* 兼容 iOS < 11.2 */
    padding-bottom: env(safe-area-inset-bottom); /* 兼容 iOS >= 11.2 */
}

.terms_management{
    padding: 32rpx 32rpx 0rpx;
}
.terms_management_content{
    background: #FFFFFF;
    padding:  0rpx 32rpx;
    border-radius: 16rpx;
}
.terms_management_item{
    display: flex;
    justify-content: space-between;
    align-items: center;
  
    padding:   32rpx 0rpx;
}


.terms_management image{
    width: 32rpx;
    height: 32rpx;
}
.terms_management_title{
    font-family: 'PingFang SC';
font-style: normal;
font-weight: 400;
font-size: 16px;
line-height: 22px;
/* 标题文字 */
color: #404040;
}

.terms_management_text{
    margin-top: 32rpx;
    font-family: 'PingFang SC';
font-style: normal;
font-weight: 400;
font-size: 14px;
line-height: 20px;
/* identical to box height */
color: #808080;
}

