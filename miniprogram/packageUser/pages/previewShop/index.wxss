  .pageHome .top_slideshow swiper swiper-item .swiper-item-content {
    position: relative;
    /* padding: 50rpx; */
    border-radius: 16rpx;
    border: 4rpx solid #7340B3;
  
    /* width: 596rpx !important; */
    /* margin:0 16rpx 0 16rpx; */
    /* background: #C4C4C4; */
    overflow: hidden;
    height: 232rpx;
    /* padding: 32rpx; */
