    box-sizing: content-box;
    transition-property: all;
    /* transition-duration: 0.5s; */
  
  }
  
  .pageHome .top_slideshow .gears {
    position: absolute;
    top: 0;
    left: 0;
    width: 80rpx;
    height: 40rpx;
    background: #7340B3;
    border-radius: 8rpx;
    font-size: 24rpx;
    text-align: center;
    line-height: 40rpx;
    /* identical to box height, or 83% */
  
  
    color: #FFFFFF;
  }
  
  .pageHome .top_slideshow swiper swiper-item .active {
    transform: scale(0.89);
    /* transition: all .2s ease-in 0s; */
    z-index: 20;
    box-sizing: border-box;
    height: 280rpx;
    transition-property: all;
    transition-duration: 0.5s;
  }
  
