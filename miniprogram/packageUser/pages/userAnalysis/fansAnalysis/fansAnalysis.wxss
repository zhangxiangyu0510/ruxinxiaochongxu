/* pages/userAnalysis/fansAnalysis/fansAnalysis.wxss *//* pages/userAnalysis/visitorsAnalysis/visitorsAnalysis.wxss */
page {
  min-height: 100%;
  background: #fafafa;
  /* padding-bottom:108rpx; */
}
.visitorsAnalysis{
  padding:32rpx;
}
.nomore{
  text-align: center;
  font-size: 24rpx;
  color:#ccc;
  margin-top:20rpx;
}
.chart1{
  position: relative;
}
.chart1 .text{
  position: absolute;
  top:45%;
  left:45%;
  line-height:30rpx;
}
.topText{
  font-family: 'PingFang SC';
  font-style: normal;
  font-weight: 300;
  font-size: 24rpx;
  color: #7F7F7F;
  margin-top:32rpx;

}
.date{
  display: flex;
  vertical-align: bottom;
}
.month{
  padding: 12rpx 20rpx;
  background: rgba(115, 64, 179, 0.1);
  border-radius: 8rpx;
  font-family: PingFang SC;
  font-style: normal;
  font-weight: normal;
  font-size: 24rpx;
  text-align: center;

/* identical to box height, or 150% */


color: #7340B3;
}
.change{
  font-family: PingFang SC;
font-style: normal;
font-weight: 300;
font-size: 24rpx;
line-height: 34rpx;
/* identical to box height */
margin-left:16rpx;

text-align: right;

color: #4A4A4A;
vertical-align: bottom;
margin-top:30rpx;
}
.wrap{
  margin-top:48rpx
}
.top{
  display: flex;
  justify-content: space-between;
  vertical-align: bottom;
}
.title{
  font-family: PingFang SC;
font-style: normal;
font-weight: 600;
font-size: 40rpx;
line-height: 56rpx;
/* identical to box height */


color: #404040;
}
.more{
  font-family: PingFang SC;
font-style: normal;
font-weight: normal;
font-size: 24rpx;
line-height: 34rpx;
color: #4A4A4A;
margin-top:20rpx;
}
.content{
  margin-top:30rpx;
  background:#fff;
  padding:60rpx;
  border-radius: 16rpx;
}
.chart1{
  width:550rpx;
  height:420rpx;
  
}
.tab{
  width:100%;
  overflow-x: scroll;
  margin-top:64rpx;
}
.tabContent{
  white-space: nowrap;
}
.tabItem{
  display: inline-block;
  margin-right:48rpx;
  font-family: PingFang SC;
  font-style: normal;
  font-weight: normal;
  font-size: 40rpx;
  line-height: 56rpx;
  color: #404040;
}
.active{
  font-family: PingFang SC;
  font-style: normal;
  font-weight: 600;
  font-size: 40rpx;
  line-height: 56rpx;
  /* identical to box height */


  color: #404040;

}
.active .line{
  display:block;


}
.line{
  width: 40rpx;
  height: 2px;
  left: 92rpx;
  background: #7340B3;
  border-radius: 14rpx;
  text-align:center;
  margin-left:calc(50% - 20rpx);
  display: none;
}
.visitItem{
  margin-top:32rpx;
  width: 630rpx;  
  background: #FFFFFF;
  border-radius: 16rpx;
  padding:32rpx;
  display: flex;

}
.awata{
  width: 100rpx;
  height: 100rpx;
  border-radius: 8rpx;
  background:#ff0;
}
.awata image{
  width:100rpx;
  height:100rpx;
}
.text{
  margin-left:12px;
 
}
.textTop{
  display: flex;

}
.textTop image{
  width:30px;
  height:30px;
}
.textBottom{
  display: flex;
}
.nickName{
  font-family: PingFang SC;
  font-style: normal;
  font-weight: 500;
  font-size: 32rpx;
  line-height: 44rpx;

  color: #333333;
}
.visitTime{
  font-family: PingFang SC;
font-style: normal;
font-weight: normal;
font-size: 24rpx;
line-height: 24rpx;
color: #333333;
}
.visitNumber{
  font-family: PingFang SC;
font-style: normal;
font-weight: normal;
font-size: 20rpx;
line-height: 28rpx;
float:right;
/* identical to box height, or 100% */
margin-left:36rpx;


color: #7F7F7F;
}