/* pages/shopkeeperLevelProcess/hierarchicalequity/hierarchicalequity.wxss */
page {
  min-height: 100%;
  box-sizing: border-box;
  background-color: #fafafa;
}
.hierarchicalequity{
    padding: 0 32rpx;
  margin-bottom: 44rpx;
  overflow: hidden;
}
.he_header {
  height: 34rpx;
  margin-top: 36rpx;
  font-size: 24rpx;
  line-height: 34rpx;

  /* identical to box height */
  border-bottom: 1px solid #e1e1e1;
  /* 标题文字 */
  color: #404040;
  padding-bottom: 16rpx;
}

.he_header_text {
  width: 120rpx;
  height: 34rpx;
  float: left;
}

.he_header_text1 {
  margin-left: 88rpx;
  float: left;
}

.he_content_item {
  height: 186rpx;
  display: flex;
  align-items: center;
}

.he_content_item_img {
  width: 116rpx;
  height: 116rpx;
}

.he_content_item_right {
  width: calc(100% - 208rpx);
  margin-left: 92rpx;
  height: calc(100% - 2rpx);
  line-height: 40rpx;
  display: flex;
  align-items: center;
  border-bottom: 1px solid #e1e1e1;
}

.he_content_item_cilcle {
  width: 8rpx;
  height: 8rpx;
  margin-top: 16rpx;
  background-color: #A6A6A6;
  border-radius: 50%;
  float: left;
}

.he_content_item_text {
  margin-left: 16rpx;
  font-size: 28rpx;

  /* identical to box height */

  /* 内容文字 */
  color: #4A4A4A;

  mix-blend-mode: normal;
  opacity: 0.9;
}