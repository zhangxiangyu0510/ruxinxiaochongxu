/* packageRecommend/pages/storeAnalysisPages/orderAmount/orderAmount.wxss */
.storeAnalysis{
    font-family: 'PingFang SC';
    font-style: normal;
    padding: 48rpx 32rpx 0;
    background-color: #fafafa;
  }
  .storeAnalysis .storeAnalysis-date{
    display: flex;
    align-items: flex-end;
    margin-bottom: 32rpx;
  }
  .storeAnalysis .storeAnalysis-date .currentDate{
    width: 156rpx;
    height: 60rpx;
    background: rgba(115, 64, 179, 0.1);
    border-radius: 8rpx;
    font-weight: 400;
    font-size: 24rpx;
    line-height: 60rpx;
    color: #7340B3;
    text-align: center;
    margin-right: 16rpx;
  }
  .storeAnalysis .storeAnalysis-date  .pickerDate{
    width: 96rpx;
  }
  .statement-title{
    font-weight: 300;
    font-size: 24rpx;
    line-height: 34rpx;
    color: #7F7F7F;
    margin-bottom: 48rpx;
  }
  .storeAnalysis .storeAnalysis-date .changeDate{
    /* width: 100%; */
    font-weight: 300;
    font-size: 24rpx;
    line-height: 34rpx;
    text-align: right;
    color: #4A4A4A;
  }
  .storeAnalysis-type{
    margin-bottom: 64rpx;
  }
  .storeAnalysis-type .type-title{
    display: flex;
    align-items: flex-end;
    margin-bottom: 32rpx;
  }
  .title-name{
    font-weight: 600;
    font-size: 40rpx;
    line-height: 56rpx;
    color: #404040;
    margin-right: 8rpx;
  }
  .title-name-rate{
    margin-right: 8rpx;
  }
  .look-detail{
    font-weight: 300;
    font-size: 24rpx;
    line-height: 34rpx;
    color: #4A4A4A;
  }
  .type-content{
    box-sizing: border-box;
    width: 100%;
    background: #FFFFFF;
    border-radius: 16rpx;
    padding: 32rpx;
  }
  .type-content .content-title .content-title-top{
    font-weight: 400;
    font-size: 24rpx;
    line-height: 34rpx;
    color: #808080;
    margin-bottom: 16rpx;
  }
  .content-title-top-people{
    font-weight: 400;
    font-size: 24rpx;
    line-height: 34rpx;
    color: #808080;
  }
  /* .content-title-bottom{
    font-weight: 400;
    font-size: 24rpx;
    line-height: 34rpx;
    color: #4A4A4A;
    margin-bottom: 28rpx;
  } */
  .content-title-bottom-people{
    font-weight: 400;
    font-size: 24rpx;
    line-height: 34rpx;
    color: #4A4A4A;
    margin-bottom: 16rpx;
  }
  .content-data{
    display: flex;
    justify-content: space-between;
  }
  .content-left{
    font-weight: 700;
    font-size: 40rpx;
    line-height: 72rpx;
    text-align: right;
    color: #7A4AB7;
  }
  .content-left-big{
    font-weight: 700;
    font-size: 72rpx;
    line-height: 72rpx;
    text-align: right;
    color: #7A4AB7;
  }
  .content-right{
    font-weight: 400;
    font-size: 24rpx;
    line-height: 34rpx;
    text-align: right;
    color: #808080;
  }
  .right-top{
    margin-bottom: 8rpx;
  }
  .right-top,.right-bottom{
    display: flex;
  }
  .img{
    position: relative;
    top: 5rpx;
    width: 24rpx;
    height: 24rpx;
  }
  .img image{
    width: 100%;
    height: 100%;
  }
.content-title-list{
    display: flex;
    justify-content: space-between;
    font-weight: 400;
    font-size: 28rpx;
    line-height: 40rpx;
    color: #4A4A4A;
    margin-bottom: 24rpx;
}
.total-amount{
    font-weight: 700;
    font-size: 40rpx;
    line-height: 40rpx;
    text-align: right;
    color: #7340B3;
}
  .title-list{
    display: flex;
    align-items: flex-end;
  }
  .total{
    font-weight: 400;
    font-size: 20rpx;
    line-height: 28rpx;
    color: #808080;
    margin-left: 16rpx;
  }
  .margin-bottom-16{
      margin-bottom: 32rpx;
  }
  .content-data-list{
      display: flex;
      /* justify-content: space-between; */
  }
  .img-list{
      width: 144rpx;
      height: 144rpx;
      margin-right: 15rpx;
  }
  .img-list:last-child{
      margin: 0;
  }
  .img-list image{
      width: 100%;
      height: 100%;
  }
  .notData{
    width: 100%;
    text-align: center;
    color: #999999;
}
  