/* pages/ucenter/replaceProduct/replaceProduct.wxss */
.replace_product {
  background-color: #fafafa;
  /* padding: 0 32rpx; */
  /* height: 100vh; */
  box-sizing: border-box;
  padding-bottom: 194rpx;
  /* margin-bottom: 194rpx; */

}

.replace_product .tio_text {
  font-family: PingFang SC;
  font-style: normal;
  font-weight: normal;
  font-size: 28rpx;
  line-height: 40rpx;
  /* identical to box height */
  text-align: center;
  color: #404040;
  padding-top: 40rpx;
  padding-bottom: 32rpx;
}

.replace_product swiper {
  height: 280rpx;

}

.replace_product swiper swiper-item {
  width: 596rpx !important;
  border-radius: 16rpx;
  overflow: hidden;

  /* padding: 0 32rpx; */
  /* box-sizing: content-box; */
  /* margin-right: 30rpx; */

}

.replace_product swiper swiper-item .swiper-item-content {
  position: relative;
  /* padding: 50rpx; */
  border-radius: 16rpx;
  border: 4rpx solid #7340B3;

  /* width: 596rpx !important; */
  /* margin:0 16rpx 0 16rpx; */
  /* background: #C4C4C4; */
  overflow: hidden;
  height: 232rpx;
  /* padding: 32rpx; */
  padding-top: 40rpx;
  padding-left: 32rpx;
  box-sizing: content-box;

}

.replace_product .gears {
  position: absolute;
  top: 0;
  left: 0;
  width: 80rpx;
  height: 40rpx;
  background: #7340B3;
  border-radius: 8rpx;
  font-size: 24rpx;
  text-align: center;
  line-height: 40rpx;
  z-index: 100;
  /* identical to box height, or 83% */


  color: #FFFFFF;
}

.replace_product swiper swiper-item .active {
  transform: scale(0.89);
  transition: all .2s ease-in 0s;
  z-index: 20;
  box-sizing: border-box;
  height: 280rpx;
}

.replace_product swiper swiper-item .shop_synopsis {
  font-weight: 600;
  font-size: 14px;
  line-height: 20px;
  color: #404040;
  width: 312rpx;
  min-height: 120rpx;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 3;
  line-clamp: 3;
  -webkit-box-orient: vertical;
}

.replace_product .retailPrice {
  /* width: 60rpx; */
  background: #888888;
  border-radius: 4rpx;
  font-size: 20rpx;
  line-height: 20rpx;
  text-align: center;
  /* identical to box height, or 100% */
  color: #FFFFFF;
  padding: 6rpx;
  margin-right: 16rpx;

}

.replace_product .retailPrice_item {
  display: flex;
  align-items: center;
  margin-top: 16rpx;
  margin-bottom: 8rpx;
  height: 32rpx;

}

.retailPrice_item .number {
  font-size: 24rpx;
  line-height: 28rpx;
  color: #888888;
  height: 28rpx;

}

.replace_product .preferentialPrice {
  /* width: 60rpx; */
  background: #7340B3;
  border-radius: 4rpx;
  font-size: 20rpx;
  line-height: 20rpx;
  text-align: center;
  /* identical to box height, or 100% */
  color: #FFFFFF;
  padding: 6rpx;
  margin-right: 16rpx;

}

.replace_product .preferentialPrice_item_box {
  display: flex;
  align-items: center;
  height: 32rpx;
  justify-content: space-between;
}

.replace_product .preferentialPrice_item {
  display: flex;
  align-items: center;
  height: 32rpx;
}

.retailPrice_item .number {
  font-size: 24rpx;
  line-height: 28rpx;
  color: #888888;
  height: 28rpx;

}

.preferentialPrice_item .number {
  font-size: 28rpx;
  line-height: 32rpx;
  /* 夏天橙色 */
  color: #7340B3;

}

.replace_product swiper swiper-item .content_item {
  display: flex;
}

.replace_product .imgBox {
  border-radius: 16rpx;
  width: 200rpx;
  height: 200rpx;
  background-color: #fff;
  margin-left: 20rpx;
  position: relative;
  top: -4rpx;
}

.replace_product .imgBox image {
  width: 200rpx;
  height: 200rpx;
  border-radius: 16rpx;

}

/* 组合推荐 */
.recommend {
  padding: 0 32rpx;
  margin-top: 80rpx;
  margin-bottom: 32rpx;
}

.recommend .top_title {
  display: flex;
  align-items: center;
  justify-content: space-between;

}

.recommend .top_title .title_img {
  width: 200rpx;
  height: 40rpx;

}

.recommend .top_title .title_img image {
  width: 200rpx;
  height: 40rpx;

}

.recommend .top_title .title_right {
  display: flex;
  align-items: center;
}


.recommend .top_title .title_right .icon_img {
  width: 20rpx;
  height: 20rpx;
}

.recommend .top_title .title_right .exit {
  font-size: 24rpx;
  line-height: 34rpx;
  /* text-align: right; */
  color: #888888;
  margin-left: 4rpx;

}

.recommend .goods_item {
  position: relative;
  background: #FFFFFF;
  border-radius: 8px;
  width: 686rpx;
  height: 264rpx;
}

.recommend .goods_box .goods_synopsis {
  font-family: PingFang SC;
  font-style: normal;
  font-weight: 600;
  font-size: 28rpx;
  line-height: 40rpx;
  color: #4A4A4A;
  width: 398rpx;
  height: 120rpx;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 3;
  line-clamp: 3;
  -webkit-box-orient: vertical;
  word-break: break-all;

}

.recommend .goods_box .goods_item {
  margin-top: 32rpx;
  padding: 32rpx;
  position: relative;
  box-sizing: border-box;

}

.goods_box .goods_item .goods_item_content {
  display: flex;
}

.goods_item_content .left_img_box {
  width: 200rpx;
  height: 200rpx;
  /* background-color: #ccc; */
  margin-right: 24rpx;
  border-radius: 8rpx;

}

.goods_item_content .left_img_box image {
  width: 200rpx;
  height: 200rpx;

}


.replace_product .replaceProduct {
  font-size: 24rpx;
  line-height: 34rpx;
  text-align: right;
  color: #7340B3;
  display: flex;
  align-items: center;
}

.replace_product .replaceProduct .icon {
  width: 32rpx;
  height: 32rpx;
  margin-right: 16rpx;

}

.replace_product .replaceProduct .icon image {
  width: 32rpx;
  height: 32rpx;
}

.replace_product .goods_item_content .retailPrice_item {
  margin-top: 8rpx;
}

.replace_product .footer_box {
  /* padding: 0 32rpx; */
  margin-bottom: 4rpx;
  box-sizing: border-box;
  margin-top: 32rpx;
  position: relative;

}

.replace_product .footer_box .footer_content {
  width: 686rpx;
  height: 264rpx;
  background: #FFFFFF;
  border-radius: 16rpx;
  padding: 48rpx;
  box-sizing: border-box;
  position: relative;



}

.replace_product .footer_box .footer_content .addBgIcon {
  width: 84rpx;
  position: absolute;
  right: 30rpx;
  bottom: 22rpx;
}

.footer_content .text {
  width: 426rpx;
  height: 80rpx;
  font-family: PingFang SC;
  font-style: normal;
  font-weight: normal;
  font-size: 28rpx;
  line-height: 40rpx;
  text-align: center;
  color: #888888;
  margin: 0 auto;
  margin-bottom: 54rpx;
}

.footer_box .footer_content .add_goods {
  display: flex;
  align-items: center;
  justify-content: center;
}

.footer_box .footer_content .add_goods .icon {

  margin-right: 20rpx;
}

.footer_box .footer_content .add_goods .icon image {
