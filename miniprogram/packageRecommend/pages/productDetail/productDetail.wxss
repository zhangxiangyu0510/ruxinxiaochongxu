.page {
    background-color: #F7F7F7;
    width: 750rpx;
    min-height: 100vh;
    overflow-x: hidden;
}

.product_img {
    width: 750rpx;
    padding-left: 16rpx;
    display: flex;
    background-color: #ffffff;
}

.imgStyle {
    /* width: 359rpx;
    height: 330rpx; */
    width: 640rpx;
    height: 640rpx;
    margin: 0 auto;
}

.product_body {
    background-color: #ffffff;
    margin: 32rpx;
    border-radius: 8rpx;
    padding-bottom: 32rpx;
}

.body_title {
    /* width: 100%; */
    font-family: 'PingFang SC';
    font-style: normal;
    font-weight: 600;
    font-size: 42rpx;
    color: #000000;
    padding-left: 32rpx;
    padding-top: 40rpx;
    margin-bottom: 66rpx;
}

.price_info {
    position: relative;
    margin: 30rpx 0;
    display: flex;
    padding: 0 32rpx;
    left: 30rpx;
}

.price_info image {
    position: absolute;
    width: 160rpx;
    height: 70rpx;
    left: 0;
}

.price_info .text {
    position: absolute;
    left: 30rpx;
    z-index: 2;

}

.price {

    text-align: center;
    font-size: 30 rpx;
    font-family: PingFangSC-Light, PingFang SC;
    font-weight: 300;
    /* padding: 10rpx 32rpx; */
    width: 140rpx;
    height: 70rpx;
    line-height: 70rpx;
    /* margin-right: 20rpx; */
    background-repeat: no-repeat;
    background-size: cover;
    color: #a7a7a7;
    z-index: 10;

}

.member {
    color: #ffffff;

}

.price_num {
    font-size: 32rpx;
    font-family: PingFangSC-Light, PingFang SC;
    /* font-weight: 700; */
    margin-right: 20rpx;
    line-height: 70rpx;
    color: #a7a7a7;

}

.memberNum {
    color: #3b8fb3;
    font-weight: 700;


}

.price_star {
    width: 630rpx;
    height: 154rpx;
    margin: 0 auto;
    background: linear-gradient(243deg, #00C0E4 0%, #EC38A2 100%);
    border-radius: 10rpx;
    position: relative;
}

.price_star_info {
    display: flex;
    position: absolute;
    top: 0;
}

.price_star_price,
.price_star_num {
    height: 20px;
    font-size: 14px;
    font-family: PingFangSC-Semibold, PingFang SC;
    font-weight: 600;
    color: #FFFFFF;
    line-height: 20px;
}

.price_star_btn {
    width: 186rpx;
    height: 60rpx;
    background: #FFFFFF;
    border-radius: 38rpx;
    position: absolute;
    right: 20rpx;
    top: 24rpx;
    font-size: 24rpx;
    font-family: PingFangSC-Medium, PingFang SC;
    font-weight: 500;
    color: #008AB0;
    line-height: 60rpx;
    text-align: center;
}

.price_star_bottom {
    width: 614rpx;
    height: 48rpx;
    line-height: 48rpx;
    position: absolute;
    bottom: 0;
    background: linear-gradient(90deg, #EEEEEE 0%, #FFFFFF 50%, #EAFAFF 100%);
    border-radius: 10rpx 10rpx 0 0;
    opacity: 0.55;
    backdrop-filter: blur(10px);
    font-size: 20rpx;
    color: #bababb;
    font-family: PingFangSC-Regular, PingFang SC;
    font-weight: 400;
    padding-left: 16rpx;
}

.product_bottom {
    background: #FFFFFF;
    border-radius: 16rpx;
    margin: 32rpx;
    padding: 32rpx;
    /* height: 254rpx; */
}

.title {
    font-family: 'PingFang SC';
    font-style: normal;
    font-weight: 600;
    font-size: 32rpx;
    color: #000000;
    margin-bottom: 32rpx;
}

.detail_list {
    display: flex;
    justify-content: space-between;
    align-items: center;
    color: #50A0D5;
}

.detail_list .name {
    font-family: 'PingFang SC';
    font-style: normal;
    text-decoration-line: underline;
    font-weight: 400;
    width: 430rpx;
    text-overflow: -o-ellipsis-lastline;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    line-clamp: 2;
    -webkit-box-orient: vertical;
    margin-bottom: 20rpx;
}

.detail_list .num {
    font-family: 'PingFang SC';
    font-style: normal;
    font-weight: 400;
    flex-shrink: 0;

}

